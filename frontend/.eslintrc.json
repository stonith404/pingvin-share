{
  "extends": ["eslint-config-next", "eslint:recommended", "prettier"],
  "plugins": ["react"],
  "rules": {
    "quotes": ["warn", "double", { "allowTemplateLiterals": true }],
    "react-hooks/exhaustive-deps": ["off"],
    "import/no-anonymous-default-export": ["off"],
    "no-unused-vars": ["warn"],
    "react/no-unescaped-entities": ["off"]
  }
}
